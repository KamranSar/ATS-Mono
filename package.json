  "scripts": {
    "preinstall": "chmod +x scripts/*.sh; scripts/get_preinstall_scripts.sh",
    "serve": "vue-cli-service serve",
    "docs": "cd docs && if [ ! -d node_modules ]; then npm ci; fi && npm run dev",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "fix": "vue-cli-service lint --fix",
    "prod": "http-server -p 8090 --proxy http://localhost:8090? ./dist",
    "clean": "rm -rf *_modules package-lock.json .env gitinfo",
    "cleaninstall": "npm run clean;\nnpm install",
    "gitversion": "git log -n 1 --pretty=format:'GIT_COMMIT=%H%nGIT_HASH=%h%nGIT_DATE=%aD'",
    "start-servers": "bash ./scripts/start_test_servers.sh",
    "stop-servers": "bash ./scripts/stop_test_servers.sh",
    "restart-servers": "bash ./scripts/fast_restart.sh",
    "merge-squash": "bash ./scripts/merge_squash.sh",
    "new": "hygen new",
    "preserve": "npm run gitversion > ./gitinfo",
    "prebuild": "npm run gitversion > ./gitinfo",
    "styleguide": "vue-cli-service styleguidist",
    "styleguide:build": "vue-cli-service styleguidist:build"
  },
